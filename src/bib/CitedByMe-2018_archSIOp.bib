isbn = {978-3-540-72666-1},
issn = {0302-9743},
keywords = {ontology alignment,sem,semantic interoperability},
organization = {European Semant Syst Initiat; ST12; CTIC Fdn; DERI Innsbruck; Empolis; CERFRIEL; EU Project X Media; BIT; TRIP COM; Hanival; Ontotext; Asemantics; Joint Sch Informat Technol; KnowledgeWeb; Ontoprise},
pages = {429--442},
publisher = {SPRINGER-VERLAG BERLIN},
