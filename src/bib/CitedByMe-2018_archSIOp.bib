isbn = {978-3-642-29824-0},
pages = {IX, 106},
publisher = {Springer Berlin Heidelberg},
series = {Springer Briefs in Computer Science},
title = {{On the mathematics of modelling, metamodelling, ontologies and modelling languages}},
url = {http://link.springer.com/10.1007/978-3-642-29825-7 http://link.springer.com/10.1007/978-3-642-29825-7{\_}8 http://link.springer.com/10.1007/978-3-642-29825-7{\_}9 http://link.springer.com/10.1007/978-3-642-29825-7{\_}5 http://link.springer.com/10.1007/978-3-642-29},
